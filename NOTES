CheckBattery:
    BatteryLevel := GetBatteryLevel()
    if (BatteryLevel = -1)  ;;âˆ™------âˆ™If running on AC power.
    {
        MsgBox, 64, Power Status, The system is running on AC power.    ;;âˆ™------âˆ™Inform the user that AC power is detected., 4
        Return
    }
    if (BatteryLevel = -2)    ;;âˆ™------âˆ™If power source or battery cannot be detected.
    {
        MsgBox, 16, Power Error, Unable to detect power source or battery status.    ;;âˆ™------âˆ™Inform the user of detection failure., 4
        Return
    }
    if (BatteryLevel < 33)    ;;âˆ™------âˆ™If battery level is below 33%.
    {
        MsgBox, 48, Low Battery, Your battery level is at %BatteryLevel%`%. Please charge your laptop!, 4
    }
Return    ;;âˆ™------âˆ™End of the CheckBattery subroutine.

^D::    ;;âˆ™------âˆ™Hotkey (Ctrl+D) to check and Display the current battery level or status.
    BatteryLevel := GetBatteryLevel()
    if (BatteryLevel = -1)
        MsgBox, 64, Power Status, The system is running on AC power., 4
    else if (BatteryLevel = -2)
        MsgBox, 16, Power Error, Unable to detect power source or battery status., 4
    else
        MsgBox, 48, Battery Level, Your battery level is at %BatteryLevel%`%., 4
Return

GetBatteryLevel() {
    VarSetCapacity(Status, 1) 
    if (DllCall("Kernel32.dll\GetSystemPowerStatus", "UInt*", &Status)) {
        ACLineStatus := NumGet(Status, 0, "UChar")    ;;âˆ™------âˆ™Get AC power status (0: Battery, 1: AC Power, 255: Unknown).
        BatteryLifePercent := NumGet(Status, 2, "UChar")    ;;âˆ™------âˆ™Get battery level (percentage).
        if (ACLineStatus = 1)    ;;âˆ™------âˆ™If the system is on AC power.
            Return -1
        else if (ACLineStatus = 255)    ;;âˆ™------âˆ™If power status is unknown.
            Return -2
        Return BatteryLifePercent    ;;âˆ™------âˆ™Return the battery level if on battery.
    }
    Return -2    ;;âˆ™------âˆ™Fallback if DllCall fails entirely.
}
Return
